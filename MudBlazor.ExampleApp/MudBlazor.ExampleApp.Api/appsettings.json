{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Security.Bearer": {
    "Authority": "https://localhost:{sts_port}",
    "Audience": "api"
  },
  "Urls": {
    "MudBlazorExampleApp": "https://localhost:44378/"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:yyyy-MM-dd HH:mm:ss.fff}] [{Level:u3}] {Category:0} - {Message}{NewLine:1}{Exception}",
          "restrictedToMinimumLevel": "Information"
        }
      }
    ]
  },
  "JwtToken": {
    "Issuer": "MudBlazor.ExampleApp",
    "Audience": "MudBlazor.ExampleApp",
    "SigningKey": "SShDqOc5ZLqXyPfGP/8yHfwV7Yw4vbbfUq5ttKoJQHQ=",
    "AuthTokenExpiryTimeSpan": "02:00:00",
    "RefreshTokenExpiryTimeSpan": "3.00:00:00"
  }
}